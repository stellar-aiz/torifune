# OCR Provider: "googledocumentai" or "veryfi"
VITE_OCR_PROVIDER=googledocumentai

# Google Document AI
VITE_GOOGLE_PROJECT_ID=your-project-id
VITE_GOOGLE_LOCATION=us
VITE_GOOGLE_PROCESSOR_ID=0000000000000000
# サービスアカウントは、ファイルパスまたはJSON文字列のどちらかを指定
VITE_GOOGLE_SERVICE_ACCOUNT_FILE=./credentials/service-account.json
# VITE_GOOGLE_SERVICE_ACCOUNT_JSON={"type": "service_account", ...}

# Veryfi
VITE_VERYFI_CLIENT_ID=vrfXXXXXXXX
VITE_VERYFI_CLIENT_SECRET=your-client-secret
VITE_VERYFI_USERNAME=your-username
VITE_VERYFI_API_KEY=your-api-key
